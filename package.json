{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.9.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "dev-client": "PORT=3000 webpack-dev-server --inline --progress --debug --config ./build/webpack.dev.conf.js",
    "dev-server": "PORT=8080 ./node_modules/nodemon/bin/nodemon.js --watch './src/**/*.ts' --exec 'ts-node' ./src/server.ts",
    "dev": "concurrently --kill-others --raw \"npm run dev-server\" \"npm run dev-client\"",
    "debug": "PORT=8080 ./node_modules/nodemon/bin/nodemon.js --inspect --watch './src/**/*.ts' --exec ts-node ./src/server.js",
    "clean": "rm -rf dist && mkdir dist",
    "build-server": "./node_modules/.bin/tsc",
    "build-client": "node ./build/build.js",
    "build": "npm run clean && npm run build-server && npm run build-client && npm run migrate",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/sequelize db:seed:all",
    "migrate-clean": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "setup": "npm run migrate-clean && npm run migrate && npm run seed",
    "postinstall": "npm run build",
    "start": "node ./dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
